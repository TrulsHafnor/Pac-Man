package PACMANtest;

import javafx.scene.image.Image;
import javafx.scene.image.ImageView;

import java.io.FileInputStream;
import java.io.FileNotFoundException;

public abstract class Spøkelse {
        public int posX,posY;

        public Spøkelse(int x,int y,int høyde,int bredde,int posX,int posY){
            this.posX=posX;
            this.posY=posY;
        }
        public abstract double getX();
        public abstract double getY();
        public abstract int getPosY();
        public abstract int getPosX();
        public abstract void bevegVenstre();
        public abstract void bevegHøyre();
        public abstract void bevegOpp();
        public abstract void bevegNed();
        public abstract void setX(double x);
        public abstract void setY(int y);
        public abstract void setPosX(int x);
        public abstract void setPosY(int y);

        public abstract ImageView getSpøkelse();
    }
